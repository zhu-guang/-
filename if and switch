#include <stdio.h>

int main()
{
	float score;
	int a;
	scanf("%f",&score);
	a=(int)score/10;
	if(score>=0&&score<=100)
	{
		switch(a)
		{
			case 10:
			case 9: printf("A");break;
			case 8:printf("B");break;
			case 7:printf("C");break;
			case 6:printf("D");break;
			default:printf("E");
		}
	}
	else printf("error!"); 
	return 0;

}

